{
  "name": "address-mapper",
  "version": "1.0.3",
  "description": "A WordPress plugin that allows users to upload CSVs of addresses, and then query them using a custom endpoint of the WordPress REST API.",
  "private": true,
  "scripts": {
    "build": "webpack -p --config webpack.config.prod.js",
    "watch": "webpack --watch --config webpack.config.dev.js",
    "start": "npm run watch"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-react": "^7.8.3",
    "@prettier/plugin-php": "^0.13.0",
    "babel-loader": "^8.0.6",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "prettier": "^1.19.1",
    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^1.1.3",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.28",
    "@fortawesome/free-solid-svg-icons": "^5.13.0",
    "@fortawesome/pro-solid-svg-icons": "^5.13.0",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "axios": "^0.19.2",
    "bottleneck": "^2.19.5",
    "clsx": "^1.1.0",
    "crypto-js": "^4.0.0",
    "date-fns": "^2.11.0",
    "lodash-es": "^4.17.15",
    "papaparse": "^5.1.1",
    "prop-types": "^15.7.2",
    "rc-progress": "^2.5.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-dropzone": "^10.2.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-saga": "^1.1.3"
  },
  "browserslist": "> 0.25%, last 2 versions, not dead",
  "prettier": {
    "tabWidth": 2,
    "useTabs": false,
    "singleQuote": true,
    "braceStyle": "1tbs",
    "semi": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "arrowParens": "avoid",
    "endOfLine": "lf"
  }
}
